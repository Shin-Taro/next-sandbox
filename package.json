{
  "name": "next-sandbox",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "check-code": "run-p type-check lint",
    "type-check": "tsc --noEmit",
    "lint": "eslint \"./src/**/*.{ts,tsx}\" --max-warnings 0 --cache",
    "fix": "run-s fix-by-prettier fix-by-eslint",
    "fix-by-prettier": "prettier \"./src/**/*.{ts,tsx}\" --write --ignore-path .gitignore",
    "fix-by-eslint": "eslint \"./src/**/*.{ts,tsx}\" --fix --max-warnings 0 --cache"
  },
  "dependencies": {
    "next": "14.0.1",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8",
    "eslint-config-next": "14.0.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import-access": "^2.1.2",
    "npm-run-all2": "^6.1.1",
    "postcss": "^8.4.31",
    "postcss-nested": "^6.0.1",
    "prettier": "^3.0.3",
    "typescript": "^5"
  }
}
